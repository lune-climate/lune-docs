{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "rebuild-api-reference": "ts-node ./scripts/update-from-remote-schema.ts",
    "api-reference": "npx ../../openapi-vuepress-markdown/dist/openapi-vuepress-markdown.js -s docs/data/openapi.yml -o docs/generated -R templates/resource.md -E templates/endpoints.md -e endpoints- && cd docs/generated",
    "prettier": "prettier --check \"src/**/**/*.tsx\" && prettier --check \"src/**/*.tsx\"",
    "eslint": "eslint \"src/**/*.ts\"",
    "lint": "yarn eslint && yarn prettier && yarn unused-exports",
    "fix": "eslint --fix \"src/**/*.ts\" && prettier --write \"src/**/*.ts\"",
    "unused-exports": "yarn run ts-unused-exports tsconfig.json --ignoreFiles='docs'"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.1",
    "@docusaurus/preset-classic": "^2.0.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "lune-ui-lib": "^1.2.65",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "commander-plus": "^0.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "latest",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
