{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3005",
    "build": "yarn api-reference && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "dev-serve": "docusaurus serve --port 3005",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "api-reference": "ts-node ./scripts/update-from-remote-schema.ts && yarn fix ",
    "prettier": "prettier --check \"src/**/**/*.tsx\" && prettier --check \"src/**/*.tsx\"",
    "eslint": "eslint \"src/**/*.tsx\"",
    "lint": " yarn eslint && yarn prettier && yarn unused-exports",
    "fix": "eslint --fix \"src/**/*.tsx\" &&  eslint --fix \"src/**/**/*.tsx\" && prettier --write \"src/**/*.tsx\" && prettier --write \"src/**/**/*.tsx\"",
    "unused-exports": "yarn run ts-unused-exports tsconfig.json --ignoreFiles='docs'",
    "test": "jest"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/react": "^1.6.22",
    "@mui/material": "^5.14.0",
    "camelcase-keys": "^8.0.2",
    "clsx": "^1.2.1",
    "docusaurus-lunr-search": "^2.3.2",
    "lune-ui-lib": "1.3.59",
    "prism-react-renderer": "^2.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loadable": "^5.5.0",
    "react-player": "^2.12.0",
    "to-case": "^2.0.0",
    "worker-loader": "^3.0.8"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.18.0",
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@jest/globals": "^29.6.1",
    "@tsconfig/docusaurus": "^2.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "commander-plus": "^0.0.6",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "latest",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.6.1",
    "js-yaml": "^4.1.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-unused-exports": "^9.0.5",
    "typescript": "^5.1.6",
    "url": "^0.11.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "jest": {
    "testMatch": [
      "**/*.test.+(ts|tsx)"
    ],
    "moduleNameMapper": {
      "^@site/src/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json",
          "useESM": true
        }
      ]
    }
  }
}
